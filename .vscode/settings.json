{
    "editor.tabSize": 2,
    "code-runner.executorMap": {
      "typescript": "clear && npx ts-node --files --transpile-only",
      "python": "clear && venv/bin/python"
    },
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true,
      "source.fixAll": true
    },
    "files.eol": "\n",
}